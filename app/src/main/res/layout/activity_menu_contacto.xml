<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".Contacto_ventanas.Menu_contacto">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="0dp"
        android:alpha=".80"
        app:srcCompat="@android:color/background_light" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="61dp"
        android:layout_height="52dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/Nombre_ini_contac"
        app:srcCompat="@drawable/house" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="57dp"
        android:layout_height="51dp"
        android:layout_alignEnd="@+id/Correo_contac"
        android:layout_alignParentBottom="true"
        android:onClick="Reportes_usu"
        app:srcCompat="@drawable/network" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@color/colorPrimary"
        android:onClick="alar"
        android:alpha=".80"
        app:srcCompat="@color/colorPrimary" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="345dp"
        android:layout_height="375dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="69dp"
        android:alpha=".80"
        app:srcCompat="@color/colorPrimary" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Inicio"
        android:textColor="#0011ff"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView13"
        android:layout_marginTop="88dp"
        android:text="Nombre: "
        android:textColor="#0011ff"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/Nombre_ini_contac"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView3"
        android:layout_marginEnd="58dp"
        android:text="Pon el nombre"
        android:textColor="#0011ff"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView13"
        android:layout_alignTop="@+id/Correo_contac"
        android:text="Correo:"
        android:textColor="#0011ff"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/Correo_contac"
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="231dp"
        android:layout_toEndOf="@+id/textView3"
        android:text="Pon correo"
        android:textColor="#0011ff"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView13"
        android:layout_alignTop="@+id/Nombre_usu_contac"
        android:text="Nombre de usuario:"
        android:textColor="#0011ff"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/Nombre_usu_contac"
        android:layout_width="126dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="67dp"
        android:layout_marginTop="168dp"
        android:text="aaaa"
        android:textColor="#0011ff"
        android:textSize="18sp" />

</RelativeLayout>